<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd" >
<plist version='1.0'>
<dict>
	<key>Label</key>
	<string>io.github.chend0316.videotools.daemon</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Applications/videotools.app/Contents/MacOS/videotools-daemon</string>
	</array>
	<key>KeepAlive</key>
	<true/>
	<key>UserName</key>
	<string>root</string>
	<key>StandardErrorPath</key>
	<string>/Library/Logs/videotools.log</string>
	<key>StandardOutPath</key>
	<string>/Library/Logs/videotools.log</string>
</dict>
</plist>
